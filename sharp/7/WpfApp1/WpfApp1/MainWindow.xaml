        <TextBox x:Name="LocatinX" HorizontalAlignment="Left" Height="35" Margin="285,300,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100"/>
        <TextBox x:Name="Height" HorizontalAlignment="Left" Height="35" Margin="160,300,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90"/>
        <TextBox x:Name="Width" HorizontalAlignment="Left" Height="35" Margin="40,300,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="85"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="40,260,0,0" TextWrapping="Wrap" Text="Width" VerticalAlignment="Top" Width="85"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="165,260,0,0" TextWrapping="Wrap" Text="Height" VerticalAlignment="Top" Width="85"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="285,260,0,0" TextWrapping="Wrap" Text="LocatinX" VerticalAlignment="Top" Width="85"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="405,260,0,0" TextWrapping="Wrap" Text="LocationY" VerticalAlignment="Top" Width="85"/>
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="530,260,0,0" TextWrapping="Wrap" Text="Razmer" VerticalAlignment="Top" Width="85"/>
        <DataGrid x:Name="spisok" HorizontalAlignment="Left" Height="155" Margin="40,85,0,0" VerticalAlignment="Top" Width="590" ColumnWidth="*"/>

